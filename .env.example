# Required API Keys
DEEPSEEK_API_KEY=your_deepseek_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Optional for Replicate video generation (not used in slideshow mode)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Stock Image APIs (at least one recommended)
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here
PEXELS_API_KEY=your_pexels_api_key_here

# Optional for YouTube upload
YOUTUBE_OAUTH_TOKEN=your_youtube_oauth_token_here

# Google Custom Search API (optional - for Google Images search)
GOOGLE_CSE_API_KEY=your_google_custom_search_api_key_here
GOOGLE_CSE_ID=your_google_custom_search_engine_id_here
